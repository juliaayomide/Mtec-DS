 
public class Carro extends Veiculo {

 
	public Carro(String modelo, double valorTabela, String cor, double combustivel, double ipva) {
		super(modelo, valorTabela, cor, combustivel, ipva);
	}
 
	@Override
	   public void calcularIpva() {
	       double ipva = getValorTabela() * 0.04;
	       setIpva(ipva);
	   }
 
	
}
